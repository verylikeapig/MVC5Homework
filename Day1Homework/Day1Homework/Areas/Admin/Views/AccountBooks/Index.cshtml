@model IEnumerable<Day1Homework.Domain.AccountBook>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Categoryyy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Amounttt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dateee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Remarkkk)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Updatetime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Categoryyy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Amounttt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dateee)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Remarkkk)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Updatetime)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
